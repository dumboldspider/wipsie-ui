{
  "name": "@wipsie/ui",
  "version": "1.0.197",
  "description": "A React UI components library",
  "license": "MIT",
  "homepage": "https://github.com/dumboldspider/wipsie-ui#readme",
  "author": "Daniel Brown (dannbrown.com)",
  "bugs": {
    "url": "https://github.com/dumboldspider/wipsie-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumboldspider/wipsie-ui.git"
  },
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/wipsieui.min.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clear": "rimraf ./lib ./dist ./esm ./storybook-static",
    "watch": "yarn clear && yarn build:dev-types && yarn build:watch",
    "generate": "node ./src/utils/generate/create-component.js",
    "pre-publish": "node ./scripts/pre-publish.js",
    "build": "node ./scripts/build.js",
    "build:types": "tsc -p ./buildconfig -d --emitDeclarationOnly --outDir types",
    "build:dev-types": "tsc -p ./buildconfig -d --emitDeclarationOnly --outDir dist/esm",
    "build:webpack": "webpack --config ./buildconfig/webpack.common.js",
    "build:watch": "webpack ---config ./buildconfig/webpack.dev.js",
    "pack": "yarn run clear && yarn run build && yarn run pre-publish && npm pack",
    "publish": "yarn run pack && npm publish --access public",
    "storybook": "start-storybook -p 6006 -s ./.storybook/public",
    "storybook:build": "build-storybook -s ./.storybook/public -o storybook-static ",
    "storybook:start": "yarn run storybook:build && serve storybook-static",
    "test": "echo \"testing $0\"",
    "test:jest": "jest --config .jest.config.js --no-cache",
    "test-update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "coverage": "yarn test --coverage",
    "coverage-update": "yarn test-update --coverage",
    "coverage:codecov": "yarn test --coverage && codecov"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash.throttle": "^4.1.1",
    "styled-jsx": "^4.0.1"
  },
  "peerDependencies": {
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/cli": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.9.6",
    "@react-bootstrap/babel-preset": "^2.1.0",
    "@storybook/addon-a11y": "^6.3.9",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/addon-storysource": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/styled-jsx": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^10.0.0",
    "chalk": "^4.1.2",
    "cherry-pick": "^0.5.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "css-mediaquery": "^0.1.2",
    "enzyme": "^3.11.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "execa": "^5.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^10.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.3.1",
    "react": "latest",
    "react-dom": "latest",
    "rimraf": "^3.0.2",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "storybook": "^6.3.9",
    "style-loader": "^3.3.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.3.4",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.8.0"
  }
}
