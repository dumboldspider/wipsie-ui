{
  "name": "@wipsie/icons",
  "version": "1.0.2",
  "description": "A React UI components library",
  "license": "MIT",
  "homepage": "https://github.com/dumboldspider/wipsie-ui#readme",
  "author": "Daniel Brown (dannbrown.com)",
  "bugs": {
    "url": "https://github.com/dumboldspider/wipsie-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumboldspider/wipsie-ui.git"
  },
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/wipsieui.min.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clear": "rimraf ./lib ./dist ./esm ./types ./storybook-static",
    "watch": "yarn clear && yarn build:dev-types && yarn build:watch",
    "generate": "node ./src/utils/generate/create-component.js",
    "pre:publish": "node ./scripts/pre-publish.js",
    "build": "node ./scripts/build.js",
    "build:types": "tsc -p ./buildconfig -d --emitDeclarationOnly --outDir types",
    "build:dev-types": "tsc -p ./buildconfig -d --emitDeclarationOnly --outDir dist/esm",
    "build:webpack": "webpack --config ./buildconfig/webpack.common.js",
    "build:watch": "webpack ---config ./buildconfig/webpack.dev.js",
    "pack": "yarn run clear && yarn run build && yarn run pre:publish && npm pack"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "peerDependencies": {
    "@wipsie/ui": "*",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.7",
    "@svgr/rollup": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-url": "^3.0.1",
    "sass": "^1.49.11",
    "sass-loader": "^10.2.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.2"
  }
}
